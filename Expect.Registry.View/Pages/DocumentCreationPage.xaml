<Page x:Class="Expect.Registry.View.Pages.DocumentCreationPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Expect.Registry.View.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="DocumentCreationPage" FontSize="14" 
        FontFamily="Times New Roman">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="9*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <StackPanel x:Name="Panel" Grid.Row="0" Grid.ColumnSpan="2">

        </StackPanel>
        <Button Grid.Row="1" Margin="5" Click="GoBackButtonClick">
            Назад
        </Button>
        <Button Grid.Row="1" Grid.Column="1" Margin="5" Click="CreateDocumentButtonClick">
            Создать
        </Button>
    </Grid>
</Page>
