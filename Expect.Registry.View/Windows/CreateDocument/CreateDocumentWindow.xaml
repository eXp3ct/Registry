<Window x:Class="Expect.Registry.View.Windows.CreateDocument.CreateDocumentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Expect.Registry.View.Windows.CreateDocument"
        mc:Ignorable="d"
        Title="CreateDocumentWindow" Height="800" Width="450">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="9*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>

        <Button Grid.Column="0" Grid.Row="1" Margin="10" Name="BackButton" Click="BackButton_Click">
            Назад
        </Button>
        <Button Grid.Column="1" Grid.Row="1" Margin="10" Name="CreateButton" Click="CreateButton_Click">
            Создать
        </Button>
    </Grid>
</Window>
